{
  "name": "notestaq-pwa",
  "author": "@otlichnyy",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "latest",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "babel-jest": "^26.6.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
